package u2e10;

public class Produc_Consum {
    public static void main(String[] args) {
        Cola cola = new Cola();
        Productor p = new Productor(cola, "src/u2e10/arxiu.txt");
        Consumidor c1 = new Consumidor(cola, 1);
        Consumidor c2 = new Consumidor(cola, 2);

        p.start();
        c1.start();
        c2.start();
    }
}